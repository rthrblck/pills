{
  "name": "Pills",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "eslint js",
    "lintfix": "eslint js --fix",
    "ava": "ava",
    "jest": "jest",
    "test": "npm run lint && npm run ava",
    "test:watch": "npm run ava --watch",
    "test:full": "npm run lint && npm run ava && npm run jest"
  },
  "dependencies": {
    "@expo/vector-icons": "^4.0.0",
    "ava": "^0.19.1",
    "color": "^1.0.3",
    "lodash": "^4.17.4",
    "moment": "^2.29.2",
    "native-base": "^2.1.3",
    "prop-type": "0.0.1",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.6",
    "react-native": "^0.44.0",
    "react-native-camera": "git+https://github.com/lwansbrough/react-native-camera.git",
    "react-native-code-push": "^2.0.3-beta",
    "react-native-easy-grid": "^0.1.8",
    "react-native-modalbox": "^1.3.9",
    "react-native-navigation-redux-helpers": "^0.5.0",
    "react-native-router-flux": "^3.39.1",
    "react-native-vector-icons": "^4.1.1",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-persist": "^4.8.0",
    "redux-thunk": "^2.2.0",
    "remote-redux-devtools": "^0.5.10"
  },
  "devDependencies": {
    "babel-jest": "20.0.1",
    "babel-preset-react-native": "^1.9.2",
    "jest": "20.0.1",
    "react-test-renderer": "16.0.0-alpha.6"
  },
  "jest": {
    "preset": "react-native"
  },
  "ava": {
    "files": [
      "tests_ava/*.js",
      "js/**/*.test.js",
      "js/**/*.{test,tests}.js",
      "!**/not-this-file.js",
      "!__tests__",
      "!__tests__/*.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": true,
    "tap": false,
    "powerAssert": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
